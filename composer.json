{
	"name": "ibrand/laravel-miniprogram-poster",
	"type": "library",
	"description": "iBrand's laravel miniprogram poster tool",
	"keywords": [
		"ibrand",
		"component",
		"miniprogram",
		"poster"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "tangqi",
			"email": "tangqi@element.vip"
		}
	],
	"require": {
		"php": ">=7.0",
		"anam/phantommagick": "~2.0",
		"anam/phantomjs-linux-x86-binary": "~2.1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"orchestra/testbench": "~3.5",
		"orchestra/database": "~3.5"
	},
	"autoload": {
		"psr-4": {
			"iBrand\\Miniprogram\\Poster\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"iBrand\\Miniprogram\\Poster\\Test\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"iBrand\\Miniprogram\\Poster\\PhantoMmagickServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
